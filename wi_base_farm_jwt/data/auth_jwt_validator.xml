<odoo>

    <record id="farm_validator" model="auth.jwt.validator">
        <field name="name">farm</field>
        <field name="audience">auth_jwt_farm_api</field>
        <field name="issuer">odooconnector</field>
        <field name="signature_type">secret</field>
        <field name="secret_algorithm">HS256</field>
        <field
            name="secret_key"
        >d2VpZ2hicmlkZ2Utb2Rvb2Nvbm5lY3Rvci13aXRlY2hlbnRlcnByaXNl</field>
        <field name="user_id_strategy">email</field>
        <field name="static_user_id" ref="base.public_user" />
        <field name="partner_id_strategy">email</field>
        <field name="partner_id_required" eval="True" />
    </record>

    <record id="farm_partner_route" model="auth.jwt.route">
        <field name="name">Partner</field>
        <field name="route">/api/farm/partners</field>
        <field name="method">GET</field>
        <field name="validator_id" ref="farm_validator" />
        <field name="farm_data" eval="True" />
    </record>

