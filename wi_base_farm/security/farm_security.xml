<odoo>
    <record model="ir.module.category" id="farm_parent">
        <field name="name">Farm</field>
        <field name="sequence">45</field>
    </record>

    <record model="ir.module.category" id="farm_category">
        <field name="name">Farm</field>
        <field name="sequence">10</field>
        <field name="parent_id" ref="wi_base_farm.farm_parent" />
    </record>

    <record id="farm_planning_operation" model="res.groups">
        <field name="name">Allow planning operation system in farm</field>
        <field name="category_id" ref="base.module_category_hidden" />
    </record>

    <record id="farm_user_group" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="farm_category" />
        <field
            name="implied_ids"
            eval="[(4,ref('analytic.group_analytic_accounting'))]"
        />
    </record>

    <record id="farm_manager_group" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="farm_category" />
        <field name="implied_ids" eval="[(4,ref('wi_base_farm.farm_user_group'))]" />
        <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
    </record>

    <record model="ir.rule" id="blok_rule_muti_company">
        <field name="name">Estate_Blok : multi_company</field>
        <field name="model_id" ref="model_estate_block" />
        <field name="domain_force">
            ['|', ('company_id', '=', False), ('company_id', 'in', company_ids)]
        </field>
    </record>

