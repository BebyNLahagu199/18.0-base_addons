<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_department_form_inherit_farm" model="ir.ui.view">
        <field name="name">hr.department.form.view.inherit.farm</field>
        <field name="model">hr.department</field>
        <field name="inherit_id" ref="hr.view_department_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='color']" position="after">
                <field name="farm_data" widget="boolean_toggle" />
            </xpath>
        </field>
    </record>
</odoo>
