    <record id="seq_farm_ranfall_logs" model="ir.sequence">
        <field name="name">Farm Rainfall Logs</field>
        <field name="code">estate.rainfall.logs.name</field>
        <field name="prefix">RFL/</field>
        <field name="padding">3</field>
        <field eval="True" name="use_date_range" />
        <field name="implementation">no_gap</field>
    </record>

    <!-- Premi -->
    <record id="seq_farm_daily_premi" model="ir.sequence">
        <field name="name">Farm Premi</field>
        <field name="code">estate.daily.premi</field>
        <field name="prefix">PRD/%(year)s/%(range_month)s/</field>
