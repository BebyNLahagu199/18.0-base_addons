<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="seq_farm_seed_mutation" model="ir.sequence">
        <field name="name">Farm Seed Mutation</field>
        <field name="code">estate.seed.mutation</field>
        <field name="prefix">DRAFT/%(year)s/%(range_month)s/</field>
        <field name="padding">3</field>
        <field eval="True" name="use_date_range" />
        <field name="implementation">no_gap</field>
    </record>

    <record id="seq_farm_seed_mutation_seeding" model="ir.sequence">
        <field name="name">Farm Seed Mutation Seeding</field>
        <field name="code">estate.seed.mutation.seeding</field>
        <field name="prefix">FSMSD/%(year)s/%(range_month)s/</field>
        <field name="padding">3</field>
        <field eval="True" name="use_date_range" />
        <field name="implementation">no_gap</field>
    </record>

