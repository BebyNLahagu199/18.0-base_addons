<odoo>

    <record id="picking_01" model="estate.picking">
        <field
            name="scheduled_date"
            eval="(DateTime.today() + relativedelta(days=-4)).strftime('%Y-%m-%d')"
        />
        <field name="partner_id" ref="wi_base_farm.work_contact_twl" />
        <field name="driver">Lucas Chen</field>
        <field name="vehicle">XYZ 789</field>
        <field
            name="harvest_ids"
            model="estate.harvest"
            eval="[(6, 0, [
                obj().env.ref('wi_base_farm.estate_harvesting_01').estate_harvest_ids[0].id,
                obj().env.ref('wi_base_farm.estate_harvesting_01').estate_harvest_ids[1].id,
                obj().env.ref('wi_base_farm.estate_harvesting_02').estate_harvest_ids[0].id
            ])]"
        />
    </record>

    <record id="wiz_picking_01" model="estate.picking.wizard.unload">
        <field name="picking_id" ref="wi_base_farm_stock.picking_01" />
        <field
            name="unload_date"
            eval="(DateTime.today() + relativedelta(days=-4)).strftime('%Y-%m-%d')"
        />
        <field name="net_unload" eval="12884.5" />
        <field name="additional_weight" eval="614.7" />
    </record>

    <function
        model="estate.picking.wizard.unload"
        name="action_update_unload"
        eval="[ref('wiz_picking_01')]"
    />

