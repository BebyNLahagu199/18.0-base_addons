<odoo>

    <record id="weighbridge_validator" model="auth.jwt.validator">
        <field name="name">weighbridge</field>
        <field name="audience">auth_jwt_weighbridge_api</field>
        <field name="issuer">odooconnector</field>
        <field name="signature_type">secret</field>
        <field name="secret_algorithm">HS256</field>
        <field
            name="secret_key"
        >d2VpZ2hicmlkZ2Utb2Rvb2Nvbm5lY3Rvci13aXRlY2hlbnRlcnByaXNl</field>
        <field name="user_id_strategy">static</field>
        <field name="partner_id_strategy">email</field>
        <field name="partner_id_required" eval="False" />
    </record>

    <record id="weighbridge_route" model="auth.jwt.route">
        <field name="name">Weighbridge</field>
        <field name="route">/api/weighbridge/weighbridges</field>
        <field name="method">GET</field>
        <field name="validator_id" ref="weighbridge_validator" />
        <field name="weighbridge_data" eval="True" />
    </record>

    <record id="weighbridge_partner_route" model="auth.jwt.route">
        <field name="name">Partner</field>
        <field name="route">/api/weighbridge/partners</field>
        <field name="method">GET</field>
        <field name="validator_id" ref="weighbridge_validator" />
        <field name="weighbridge_data" eval="True" />
    </record>

    <record id="weighbridge_product_category_route" model="auth.jwt.route">
        <field name="name">Weighbridge Product Categories</field>
        <field name="route">/api/weighbridge/product/categories</field>
        <field name="method">GET</field>
        <field name="validator_id" ref="weighbridge_validator" />
        <field name="weighbridge_data" eval="True" />
    </record>

    <record id="weighbridge_product_items_route" model="auth.jwt.route">
